<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="JGomezcoelloExamenProgreso2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:JGomezcoelloExamenProgreso2"
    Title="Justin Gomezcoello">

    <FlyoutItem Icon="navegar.png" FlyoutDisplayOptions="AsMultipleItems" >

        <Tab Title="Recarga Telefonica" Icon="telefono.png">
            <ShellContent
        Title="Recarga Telefonica"
        ContentTemplate="{DataTemplate local:JGomezcoelloRecarga}"
        Icon="telefono.png" />
        </Tab>


        <Tab Title="GRID" Icon="navegar.png">
            <ShellContent
        Title="GRID"
        ContentTemplate="{DataTemplate local:JGomezcoelloGrid}"
        Icon="navegar.png" />
        </Tab>
    </FlyoutItem>


</Shell>
